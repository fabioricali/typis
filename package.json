  "bugs": {
    "url": "https://github.com/fabioricali/typis/issues"
  },
  "homepage": "https://github.com/fabioricali/typis#readme",
  "devDependencies": {
    "bejs": "^1.11.0",
    "coveralls": "^2.13.1",
    "istanbul": "^0.4.5",
    "jsdoc-to-markdown": "^3.0.0",
    "mocha": "^3.5.0",
    "mocha-lcov-reporter": "^1.3.0"
  }
